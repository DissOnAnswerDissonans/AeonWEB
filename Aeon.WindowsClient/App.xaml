<Application x:Class="Aeon.WindowsClient.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:Aeon.WindowsClient"
             xmlns:vm="clr-namespace:Aeon.WindowsClient.ViewModels"
             xmlns:sr="clr-namespace:Microsoft.AspNetCore.SignalR.Client;assembly=Microsoft.AspNetCore.SignalR.Client.Core"
             xmlns:b="clr-namespace:Aeon.Base;assembly=Aeon.Base"
             xmlns:sys="clr-namespace:System;assembly=mscorlib"
             StartupUri="MainWindow.xaml"
             >
  <Application.Resources>
    <ResourceDictionary>
      <ResourceDictionary.MergedDictionaries>
        <ResourceDictionary Source="/Resources/Wpf/luna.normalcolor.xaml"/>
        <ResourceDictionary Source="/Resources/Settings.xaml"/>
        <ResourceDictionary Source="/Resources/Theme.xaml"/>
        <ResourceDictionary Source="/Resources/Sprites.xaml"/>
      </ResourceDictionary.MergedDictionaries>
    </ResourceDictionary>
  </Application.Resources>
</Application>
